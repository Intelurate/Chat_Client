{
    "name": "Page Swarm Online",
    "description": "Change the current page color",
    "version": "1.0.1.1",
    "permissions": [
        "tabs", "http://*/*", "https://*/*",
        "storage",
        "identity"
    ],
    "background": {
        "scripts": ["js/md5.js", "js/background.js", "js/underscore.js"],
        "persistent": false
    },  
    "browser_action": {
        "default_title": "Set this page's color.",
        "default_icon": "icon.png"
    },
    "manifest_version": 2,
    "web_accessible_resources": [
        "css/fonts/*",
        "images/emotocons/*.png",
        "images/*.png",
        "js/workers/test.js",
        "templates/*"
    ]  
}
