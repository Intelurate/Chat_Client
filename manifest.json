{
    "name": "Page Swarm Online",
    "description": "Page Swarm Online",
    "version": "1.0.1.9",
    "permissions": [
        "tabs", "http://*/*", "https://*/*",
        "storage",
        "identity"
    ],
    
    "content_scripts": [{        
        "matches": ["http://*/*", "https://*/*"],
        "exclude_matches": [
            "https://eddie.openpeak.org/*",
            "http://techcrunch.com/page/*", 
            "https://www.google.com/*", "https://mail.google.com/*"
        ],
        "css":[
            "css/ps_icon.css"],
        "js":[
            "js/jquery.js",
            "js/loadApp.js"
        ]        
    },   
    {
        "matches": [
        "https://www.google.com/*"
        ],
        "css":[
            "css/google_search.css"],
        "js":[
            "js/jquery.js",
            "js/underscore.js", 
            "js/socketio.js", 
            "js/md5.js", 
            "js/portals/googleSearchConnections.js"]
    }],
    "background": {
        "scripts": [
        "js/socketio.js",
        "js/underscore.js",
        "js/md5.js",
        "js/background.js"],
        "persistent": false
    },  
    "browser_action": {
        "default_title": "Set this page's color.",
        "default_icon": "icon.png",
        "default_popup": "app_menu.html"        
    },
    "manifest_version": 2,
    "web_accessible_resources": [
        "css/fonts/*",
        "images/emotocons/*.png",
        "images/*.png",
        "js/workers/test.js",
        "templates/*"
    ]  
}